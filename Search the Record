#include <stdio.h>
#include <conio.h>
#include <windows.h>
#include <stdlib.h>
int main()
{
    int i, rn[10], tm[10], f, sn;
    for(i=0; i<10;i++)
    {
        printf("Enter Roll No.: ");
        scanf("%d", &rn[i]);

        printf("Enter Total Marks");
        scanf("%d", &tm[i]);
    }
    system("cls");
    printf("Enter No. To be Searched: ");
    scanf("%d", &sn);

    f=0;
    for(i=0; i<10; i++)
    {
        if(rn[i]==sn)
        {
            f=1;
            printf("Roll No.: %d\n", rn[i]);
            printf("Total Marks: %d", tm[i]);
            break;
        }
    }
    if(f==0)
        printf("Not Found");
}
